@model List<Student>
@{
    ViewData["Title"] = "Student Information";
    Layout = "_Simple";
}

<table class="table table-dark table-hover table-sm">
    <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col">Id</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var student in Model)
        {
            <tr>
                <td>@student.Name</td>
                <td>@student.UniqueId</td>
            </tr>
        }
    </tbody>
</table>


